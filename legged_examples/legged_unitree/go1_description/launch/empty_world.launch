<launch>
    <arg name="robot_type" default="go1" doc="Robot type: [a1, aliengo, go1, laikago]"/>
    <arg name="z_pos" default="0.5" doc="initial position in gazebo"/>

    <!-- <param name="legged_robot_description" command="$(find xacro)/xacro $(find go1_description)/xacro/robot.xacro
       robot_type:=$(arg robot_type)
    "/>
    <node name="generate_urdf" pkg="legged_common" type="generate_urdf.sh" output="screen"
          args="$(find go1_description)/xacro/robot.xacro $(arg robot_type)"/> -->
<!-- 
    <rosparam file="$(find legged_gazebo)/config/default.yaml" command="load"/> -->



    <param name="legged_robot_description" command="cat /home/xuanqi/sirius_mini.urdf"/>        
    <rosparam file="$(find legged_gazebo)/config/default.yaml" command="load"/>
        
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find legged_gazebo)/worlds/empty_world.world"/>
    </include>        <!-- push robot_description to factory and spawn robot in gazebo -->
        
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" clear_params="true"
        args="-z $(arg z_pos) -param legged_robot_description -urdf -model $(arg robot_type)
    -J LF_HAA -0.00
    -J LF_HFE 0.7
    -J LF_KFE -1.5
    -J LH_HAA -0.00
    -J LH_HFE 0.7
    -J LH_KFE -1.5
    -J RF_HAA 0.00
    -J RF_HFE 0.5
    -J RF_KFE -1.5
    -J RH_HAA 0.00
    -J RH_HFE 0.5
    -J RH_KFE -1.5
    -J joint1 0.0
    -J joint2 0.0
    -J joint3 0.0"
        output="screen"/>

    <!-- push robot_description to factory and spawn robot in gazebo
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" clear_params="true"
          args="-z 0.5 -param legged_robot_description -urdf -model $(arg robot_type)" output="screen"/> -->

</launch>
